<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/eucip</property>
    <property name="hibernate.connection.username">eucip</property>
    <property name="hibernate.connection.password">eucip</property>
    <mapping resource="es/munvi/hibernateconfig/Clientes.hbm.xml"/>
    <mapping resource="es/munvi/hibernateconfig/Distribucion.hbm.xml"/>
    <mapping resource="es/munvi/hibernateconfig/Concesionarios.hbm.xml"/>
    <mapping resource="es/munvi/hibernateconfig/Coches.hbm.xml"/>
    <mapping resource="es/munvi/hibernateconfig/Marco.hbm.xml"/>
    <mapping resource="es/munvi/hibernateconfig/Ventas.hbm.xml"/>
    <mapping resource="es/munvi/hibernateconfig/Marcas.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
